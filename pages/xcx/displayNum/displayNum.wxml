        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell">
                <view class="weui-cell__bd">伞的分布地点</view>
                <view class="weui-cell__ft">可借数量</view>
            </view>
        </view>
       
          <view class="weui-cells weui-cells_after-title" wx:for="{{numberArr}}" wx:key="{{code}}">
            <view class="weui-cell">
                <view class="weui-cell__bd">{{item.location}}</view>
                <view class="weui-cell__ft">{{item.number}}</view>
            </view>
            
          
              <view wx:if="{{item.sponsor.length!=0}}" class="weui-cell weui-media-box_appmsg"  style="color:#00a82f;text-align:center" data-sp="{{item.sponsor.id}}"  bindtap="toSponsor"> 
                <view style="margin-right:30rpx;">
                    <image style="width:50rpx;height:50rpx;border-radius:50%" src="{{uploads_url}}/{{item.sponsor.logo}}"></image>
                </view>
                <view class="" >
                    <text>{{item.sponsor.description}}</text>
                </view>
                  
            </view> 
        </view>
