/* translationDetail.wxss */



.container {
  background-color: #F2f2f2;
  height: 100%;


}
.item {
  margin-top:20rpx;
  margin-left:20rpx;
  margin-right:20rpx;
  padding: 10px;
  background-color: #fff;
  color: #787878;
  border-radius:10rpx;
  /*border-top: 1px solid #eaeaea;
  border-bottom: 1px solid #eaeaea;*/
}

.item_left {
  flex: 1;

  padding-top: 0rpx;
  overflow: hidden;
}

.item_left .title {
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
  font-size: 16px;
  color: #333;
}
.item_left .sub_title {
  font-size: 14px;
  color: #666;
  margin: 2px 0px;
}
.item_left_up{
  margin-left: 5rpx;
  margin-top:15rpx;
}
.item_left_down{
  margin-top:30rpx;
  margin-left: 5rpx;

}
.item_left .up{
  display: inline-block;
  padding: 3rpx 3rpx;
  margin-right: 10rpx;


}
.item_left_up1{
  width:30rpx;
  height:30rpx;
}
.item_left_up3{
width:30rpx;
}

.item_right {
  width:190rpx;
  height:110rpx;
  position: relative;
}
.item_right_arrow{
  position: absolute;
  top: 50%;
  left: 80%;
  width:20rpx;height:30rpx

}

.text{
  font-family: SimHei;
 font-size: 30rpx;
}
.tab_title{
font-family: SimHei;
font-size: 30rpx;
}
.key_text{
  color:#f60;
}

